{
  "name": "webpack-boilerplate",
  "version": "1.0.0",
  "description": "Pubhunt boilerplate",
  "main": "index.js",
  "scripts": {
    "a11y": "npm run a11y",
    "jslint": "eslint ./src --ignore-pattern vendor/*.js *spec.js --cache",
    "csslint": "stylelint ./src/scss --fix --config .stylelintrc",
    "lint": "npm run csslint && npm run jslint",
    "clean": "rm -rf build/*",
    "build:dev": "cross-env NODE_ENV=development webpack --mode development",
    "build:prod": "cross-env NODE_ENV=production webpack --mode production",
    "start": "webpack-dev-server --mode development",
    "typecheck": "tsc"
  },
  "author": "arakno",
  "license": "MIT",
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "git://github.com/arakno/webpack-boilerplate.git"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "^7.8.3",
    "autoprefixer": "^9.7.4",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.0",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "pa11y": "^5.3.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "stylelint": "^13.2.1",
    "typescript": "^3.7.5",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "keywords": [
    "babel",
    "boilerplate",
    "es6",
    "sass",
    "starter-kit",
    "workflow",
    "webpack"
  ]
}
